<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UsuarioController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="btnEditar" layoutX="196.0" layoutY="163.0" mnemonicParsing="false" onAction="#irAEditarUsuario" text="Editar" />
            <Button fx:id="btnEliminarUsuario" layoutX="381.0" layoutY="163.0" mnemonicParsing="false" onAction="#eliminarUsuario" text="Eliminar" />
            <Label fx:id="lblSaludoUsuario" layoutX="266.0" layoutY="66.0" text="Label">
               <font>
                  <Font size="20.0" />
               </font></Label>
            <Button fx:id="btnInicio" layoutX="289.0" layoutY="229.0" mnemonicParsing="false" onAction="#irAInicio" text="Inicio" />
            <Button fx:id="btnActividades" layoutX="269.0" layoutY="289.0" mnemonicParsing="false" onAction="#irAActivdades" text="Actividades" />
         </children>
    </AnchorPane>
  </children>
</VBox>
