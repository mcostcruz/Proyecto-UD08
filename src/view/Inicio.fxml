<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.InicioController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="btnLogin" layoutX="129.0" layoutY="152.0" mnemonicParsing="false" onAction="#irALogin" text="Login" />
            <Button fx:id="btnRegistro" layoutX="427.0" layoutY="152.0" mnemonicParsing="false" onAction="#irARegistro" text="Registrarse" />
            <Label layoutX="277.0" layoutY="70.0" text="Login/SignUp" />
            <Button fx:id="btnPanelUsuario" layoutX="242.0" layoutY="218.0" mnemonicParsing="false" onAction="#irAPanel" text="Ir al panel de usuario" />
            <Label fx:id="lblUsuarioLoggeado" layoutX="14.0" layoutY="14.0" text="No hay usuario loggeado">
               <font>
                  <Font size="11.0" />
               </font>
            </Label>
      </children>
    </AnchorPane>
  </children>
</VBox>
